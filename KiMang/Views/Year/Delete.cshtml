@model KiMang.YEAR

@{
    ViewBag.Title = "Delete";
}
<h3>Delete Year</h3>
<p>Are you sure you want to delete this?</p>
<hr />
<div>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div>
            <div class="form-group d-flex">
                <div class=" ts col-6">
                    @Html.Label("Year ID :", htmlAttributes: new { @class = "control-label  col-md-3" })

                    @Html.EditorFor(model => model.Year_Id, new { htmlAttributes = new { @class = "col-md-8 ts", disabled = "disabled" } })
                    @Html.ValidationMessageFor(model => model.Year_Id, "", new { @class = "text-danger" })
                </div>
                <div class="ts ts col-6">
                    @Html.Label("Year Name:", htmlAttributes: new { @class = "control-label   col-md-3" })

                    @Html.EditorFor(model => model.Year_Descr, new { htmlAttributes = new { @class = "col-md-8 ts", disabled = "disabled" } })
                    @Html.ValidationMessageFor(model => model.Year_Descr, "", new { @class = "text-danger" })
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 float-right ">
                <input type="submit" value="Back to List" class="btn btn-danger">
                <input type="submit" value="Delete" class="btn btn-success">
            </div>
        </div>
    }
</div>
